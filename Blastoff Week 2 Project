}
</style>
<script type= "text/javascript">
var countDown;

function countDownClock() {
  var timeLeft = 10;
  var text = document.getElementById("countDownTimer");
  countDown = setInterval(function() {
    if (timeLeft == 0) {
      clearInterval(countDown);
      text.innerHTML = "Blastoff";
    } else {
      text.innerHTML = timeLeft;
    }
    timeLeft -= 1;
  }, 1000);
}
</script>
